<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:chatMamber="clr-namespace:ZappChat_v3.Core.ChatElements">
    <HierarchicalDataTemplate x:Key="GroupTemplate" DataType="{x:Type chatMamber:Group}" ItemsSource="{Binding Friends}">
        <TextBlock Text="{Binding}"/>
    </HierarchicalDataTemplate>
    <DataTemplate x:Key="FriendInsideTemplate" DataType="{x:Type chatMamber:Friend}">
        <TextBlock Text="{Binding}"/>
    </DataTemplate>
</ResourceDictionary>